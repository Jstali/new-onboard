    min_experience integer DEFAULT 0,
    max_experience integer,
    required_skills text[],
    preferred_skills text[],
    posted_date date DEFAULT CURRENT_DATE,
    closing_date date,
    filled_date date,
    hiring_manager_id integer,
    assigned_recruiter_id integer,
    created_at timestamp without time zone DEFAULT CURRENT_TIMESTAMP,
    updated_at timestamp without time zone DEFAULT CURRENT_TIMESTAMP,
    created_by integer,
    CONSTRAINT valid_employment_type CHECK (((employment_type)::text = ANY ((ARRAY['Full-Time'::character varying, 'Contract'::character varying, 'Intern'::character varying, 'Part-Time'::character varying])::text[]))),
    CONSTRAINT valid_experience CHECK (((min_experience >= 0) AND ((max_experience IS NULL) OR (max_experience >= min_experience)))),
    CONSTRAINT valid_priority CHECK (((priority)::text = ANY ((ARRAY['Low'::character varying, 'Medium'::character varying, 'High'::character varying, 'Critical'::character varying])::text[]))),
    CONSTRAINT valid_status CHECK (((status)::text = ANY ((ARRAY['Open'::character varying, 'Closed'::character varying, 'On Hold'::character varying, 'Filled'::character varying, 'Cancelled'::character varying])::text[])))
);


ALTER TABLE public.recruitment_requisitions OWNER TO postgres;

