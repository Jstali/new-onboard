--
-- TOC entry 281 (class 1259 OID 50767)
-- Name: attendance_summary; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW public.attendance_summary AS
 SELECT u.id AS employee_id,
    u.first_name,
    u.last_name,
    u.email,
    em.employee_id AS emp_id,
    em.department,
    em.designation,
    a.date,
    a.status,
    a.clock_in_time,
    a.clock_out_time,
    a.reason,
    a.updated_at
   FROM ((public.attendance a
     JOIN public.users u ON ((a.employee_id = u.id)))
     LEFT JOIN public.employee_master em ON (((u.email)::text = (em.company_email)::text)))
  ORDER BY a.date DESC, u.first_name, u.last_name;


ALTER VIEW public.attendance_summary OWNER TO postgres;

