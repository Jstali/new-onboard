    CONSTRAINT valid_breakdown_type CHECK (((breakdown_type)::text = ANY ((ARRAY['age'::character varying, 'tenure'::character varying, 'gender'::character varying])::text[]))),
    CONSTRAINT valid_percentage CHECK (((percentage >= (0)::numeric) AND (percentage <= (100)::numeric)))
);


